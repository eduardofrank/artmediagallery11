        height = 114
        width = 235
        alt = Artmedia Gallery
        linktitle = artmediagallery
    }
    favicon {
        file = EXT:artmediagallery/Resources/Public/Icons/favicon.ico
    }
    fluidtemplate {
        layoutRootPath = EXT:artmediagallery/Resources/Private/Layouts/Page/
        partialRootPath = EXT:artmediagallery/Resources/Private/Partials/Page/
        templateRootPath = EXT:artmediagallery/Resources/Private/Templates/Page/
    }
}


